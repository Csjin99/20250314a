package a.basic;

public class Output {
	public static void main(String[] args) {
		//코드를 사용하는 영역
		
		
		
		//출력문
		//println() = \n -> 개행문자를 포함
		System.out.println("안녕하세여ㅛ");
		//print() -> 일반출력문
		System.out.print("성진");
		
		//문자열 포멧을 이용한 출력문
		String name = "최성진";
		System.out.println("저는" + name);
		System.out.printf(name);
	}

}
